{
  "name": "FortDox",
  "version": "1.0.2",
  "main": "main.js",
  "scripts": {
    "start": "node main.js",
    "forever": "forever start main.js",
    "dev": "nodemon --inspect main.js --dev",
    "migrate": "sequelize db:migrate",
    "test":"jest",
    "integrationtest": "node tests/integration_tests/testrunner.js",
    "postinstall": "node -e \"var s='../server_modules',d='node_modules/app',fs=require('fs');fs.stat(d,function(e){!e||fs.symlinkSync(s,d,'dir')}); try { fs.mkdirSync('./logs') } catch(e) { console.log(e)}\""
  },
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.3",
    "client-sessions": "^0.8.0",
    "cron": "^1.2.1",
    "elasticsearch": "^13.1.1",
    "express": "^4.0.0",
    "forever": "^0.15.3",
    "moment": "^2.18.1",
    "mysql2": "^1.3.5",
    "node-schedule": "^1.2.3",
    "nodemailer": "^4.0.1",
    "sequelize": "^4.1.0",
    "uuid": "^3.1.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "@edgeguideab/expect": "^1.7.9",
    "babel-eslint": "^7.2.3",
    "eslint": "^4.19.1",
    "eslint-plugin-jasmine": "^2.10.1",
    "flags": "^0.1.3",
    "jest": "^23.4.0",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "sequelize-cli": "^2.8.0",
    "syncprompt": "^2.0.0"
